{
  "compilerOptions": {
    "sourceMap": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/site/client/components/*"],
      "pages/*": ["src/site/client/pages/*"],
    }
  },
  "include": [
    "src/site/client"
  ]
}